{
    "extends": "tslint-config-airbnb",
    "rules": {
        "import-name": false,
        "ter-indent": [true, 4],
        "trailing-comma": false,
        "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
        "max-line-length": [
            true,
            200
        ],
        "ter-arrow-parens": false
    }
}
